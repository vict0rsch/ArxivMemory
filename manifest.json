{
  "name": "Paper Memory",
  "version": "0.4.5",
  "manifest_version": 2,
  "description": "Automatically record papers you read from Arxiv, OpenReview & more! Organize your library with tags, links to code and quick notes.",
  "homepage_url": "https://github.com/vict0rsch/PaperMemory",
  "icons": {
    "192": "icons/favicon-192x192.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "src/shared/min/utils.min.js",
      "src/background/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/favicon-192x192.png",
    "default_title": "PaperMemory",
    "default_popup": "src/popup/min/popup.min.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "downloads",
    "downloads.open"
  ]
}